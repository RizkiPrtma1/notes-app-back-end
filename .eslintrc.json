{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" : "off",
        "quotes": [0, "single", { "avoidEscape": true }],
        "allowTemplateLiterals": true
    }
}
